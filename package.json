{
  "name": "postgrest-lite",
  "version": "0.1.0",
  "description": "PostgREST-compatible query interface for SQLite, optimized for Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run tests/e2e",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "dev": "tsx src/local/index.ts",
    "dev:example": "tsx examples/local-server.ts",
    "dev:auth-demo": "tsx examples/auth-rls-demo.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "postgrest",
    "sqlite",
    "cloudflare-workers",
    "supabase",
    "rest-api",
    "query-builder"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.5",
    "bcryptjs": "^3.0.2",
    "hono": "^4.10.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@supabase/supabase-js": "^2.76.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.17.10",
    "@vitest/coverage-v8": "^2.1.8",
    "better-sqlite3": "^11.8.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
